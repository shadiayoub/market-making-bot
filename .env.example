# LBank API Credentials
# Copy this file to .env and fill in your actual credentials

# API Key from LBank
LBANK_API_KEY=your_api_key_here

# API Secret from LBank (hex string for HMACSHA256, or RSA private key for RSA)
LBANK_API_SECRET=your_api_secret_here

# Sign method: "HMACSHA256" (recommended) or "RSA"
LBANK_SIGN_METHOD=HMACSHA256

# Base URL (usually https://api.lbkex.com/)
LBANK_BASE_URL=https://api.lbkex.com/

# Trading Pair Configuration
TRADING_PAIR=acces_usdt
TOKEN_SYMBOL=acces
